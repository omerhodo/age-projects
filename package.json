{
  "name": "age-projects",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:production": "cp .env.production.template .env.local && next dev --turbopack",
    "build": "next build",
    "build:production": "cp .env.production.template .env.production.local && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist build",
    "reinstall": "rm -rf node_modules yarn.lock && yarn install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "android:build": "npx cap build android",
    "android:run": "npx cap run android",
    "android:sync": "npx cap sync android",
    "android:release": "cd android && ./gradlew bundleRelease",
    "ios:build": "npx cap build ios",
    "ios:run": "npx cap run ios",
    "ios:sync": "npx cap sync ios",
    "ios:open": "npx cap open ios",
    "mobile:build": "npm run build && npx cap sync",
    "release:android": "npm run build && npx cap sync android && cd android && ./gradlew bundleRelease",
    "release:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "format:staged": "prettier --write --list-different",
    "build:mobile": "npm run build && npx cap sync",
    "ios": "npx cap run ios",
    "android": "npx cap run android",
    "add:ios": "npx cap add ios",
    "add:android": "npx cap add android",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android"
  },
  "dependencies": {
    "@capacitor-community/admob": "^7.0.3",
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.9.0",
    "date-fns": "^4.1.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "next": "15.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "typescript": "^5"
  }
}